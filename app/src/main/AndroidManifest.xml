<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="poapper.android.tutorial">

    <!-- 인터넷 사용 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".Week1.Week1_MainActivity"/>
        <activity android:name=".Week1.Week1_JoinActivity"/>
        <activity android:name=".Week1.Week1_SubActivity"/>

        <activity android:name=".Week2.Week2_LoginActivity"/>
        <activity android:name=".Week2.Week2_SubActivity"/>

        <activity android:name=".Week3.Week3_MainActivity"/>
        <activity android:name=".Week3.Week3_SubActivity"/>

        <activity android:name=".Week4.Week4_MainActivity"/>

        <activity android:name=".Week5.Week5_MainActivity"/>
    </application>

</manifest>